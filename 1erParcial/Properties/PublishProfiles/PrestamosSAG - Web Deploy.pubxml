    <InstallAspNetCoreSiteExtension>False</InstallAspNetCoreSiteExtension>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$PrestamosSAG</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ConStr" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:labsoft.database.windows.net,1433;Initial Catalog=SAGDB;Persist Security Info=False;User ID={eudy};Password={Wandayeudes1};Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:labsoft.database.windows.net,1433;Initial Catalog=SAGDB;Persist Security Info=False;User ID={eudy};Password={Wandayeudes1};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.\SqlExpress;Initial Catalog=SAGDB;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\ConStr_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.\SqlExpress; &#xD;&#xA;     &#xD;&#xA;    &#xD;&#xA;      Initial Catalog=SAGDB;  &#xD;&#xA;         Integrated Security=True" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="ConStr-Web.config Connection String">
      <ParameterValue>Server=tcp:labsoft.database.windows.net,1433;Initial Catalog=SAGDB;Persist Security Info=False;User ID={eudy};Password={Wandayeudes1};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;</ParameterValue>
